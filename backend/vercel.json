{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.js"
      },
      {
        "src": "/webhook",
        "dest": "server.js"
      },
      {
        "src": "/digiwallet/transaction/",
        "dest": "transaction.routes.js"
      },
      {
        "src": "/digiwallet/user/",
        "dest": "user.routes.js"
      }
    ]
  }